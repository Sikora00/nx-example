{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "angularapp": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/angularapp",
      "sourceRoot": "apps/angularapp/src",
      "prefix": "nx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angularapp",
            "index": "apps/angularapp/src/index.html",
            "main": "apps/angularapp/src/main.ts",
            "polyfills": "apps/angularapp/src/polyfills.ts",
            "tsConfig": "apps/angularapp/tsconfig.app.json",
            "assets": [
              "apps/angularapp/src/favicon.ico",
              "apps/angularapp/src/assets"
            ],
            "styles": ["apps/angularapp/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angularapp/src/environments/environment.ts",
                  "with": "apps/angularapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angularapp:build",
            "proxyConfig": "apps/angularapp/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "angularapp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angularapp:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angularapp/tsconfig.app.json",
              "apps/angularapp/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/angularapp/jest.config.js",
            "tsConfig": "apps/angularapp/tsconfig.spec.json",
            "setupFile": "apps/angularapp/src/test-setup.ts"
          }
        }
      }
    },
    "angularapp-e2e": {
      "root": "apps/angularapp-e2e",
      "sourceRoot": "apps/angularapp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angularapp-e2e/cypress.json",
            "tsConfig": "apps/angularapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "angularapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angularapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/angularapp-e2e/tsconfig.e2e.json"
          }
        }
      }
    },
    "reactapp": {
      "root": "apps/reactapp",
      "sourceRoot": "apps/reactapp/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/reactapp",
            "index": "apps/reactapp/src/index.html",
            "main": "apps/reactapp/src/main.tsx",
            "polyfills": "apps/reactapp/src/polyfills.ts",
            "tsConfig": "apps/reactapp/tsconfig.app.json",
            "assets": [
              "apps/reactapp/src/favicon.ico",
              "apps/reactapp/src/assets"
            ],
            "styles": ["apps/reactapp/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/reactapp/src/environments/environment.ts",
                  "with": "apps/reactapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "reactapp:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "reactapp:build:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/reactapp/tsconfig.app.json",
              "apps/reactapp/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/reactapp/jest.config.js",
            "tsConfig": "apps/reactapp/tsconfig.spec.json"
          }
        }
      }
    },
    "reactapp-e2e": {
      "root": "apps/reactapp-e2e",
      "sourceRoot": "apps/reactapp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/reactapp-e2e/cypress.json",
            "tsConfig": "apps/reactapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "reactapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "reactapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/reactapp-e2e/tsconfig.e2e.json"
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ui/tsconfig.lib.json",
              "libs/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "tsConfig": "libs/ui/tsconfig.spec.json"
          }
        }
      }
    },
    "nest-api": {
      "root": "apps/nest/api",
      "sourceRoot": "apps/nest/api/src",
      "projectType": "application",
      "prefix": "nest-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/nest/api",
            "main": "apps/nest/api/src/main.ts",
            "tsConfig": "apps/nest/api/tsconfig.app.json",
            "assets": ["apps/nest/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/nest/api/src/environments/environment.ts",
                  "with": "apps/nest/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "nest-api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/nest/api/tsconfig.app.json",
              "apps/nest/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/nest/api/jest.config.js",
            "tsConfig": "apps/nest/api/tsconfig.spec.json"
          }
        }
      }
    },
    "data": {
      "root": "libs/data",
      "sourceRoot": "libs/data/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/data/tsconfig.lib.json",
              "libs/data/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/data/jest.config.js",
            "tsConfig": "libs/data/tsconfig.spec.json"
          }
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/utils/tsconfig.lib.json",
              "libs/utils/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "tsConfig": "libs/utils/tsconfig.spec.json"
          }
        }
      }
    },
    "electron": {
      "root": "apps/angularapp/",
      "sourceRoot": "apps/angularapp/src",
      "projectType": "application",
      "prefix": "nx"
    },
    "ionic": {
      "root": "apps/ionic/",
      "sourceRoot": "apps/ionic/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "apps/ionic/www",
            "index": "apps/ionic/src/index.html",
            "main": "apps/ionic/src/main.ts",
            "polyfills": "apps/ionic/src/polyfills.ts",
            "tsConfig": "apps/ionic/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/@ionic/angular/dist/ionic/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              {
                "input": "apps/ionic/src/theme/variables.scss"
              },
              {
                "input": "apps/ionic/src/global.scss"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ionic/src/environments/environment.ts",
                  "with": "apps/ionic/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "ci": {
              "progress": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ionic:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ionic:build:production"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ionic:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/ionic/src/test.ts",
            "polyfills": "apps/ionic/src/polyfills.ts",
            "tsConfig": "apps/ionic/tsconfig.spec.json",
            "karmaConfig": "apps/ionic/karma.conf.js",
            "styles": ["apps/ionic/src/styles.css"],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ionic/tsconfig.app.json",
              "apps/ionic/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "ionic:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ionic:build:production"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "ionic:ionic-cordova-build",
            "devServerTarget": "ionic:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "ionic:ionic-cordova-build:production",
              "devServerTarget": "ionic:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "angularapp"
}
